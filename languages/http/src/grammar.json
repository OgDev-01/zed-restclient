{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "http",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "request"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "request_separator"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    },
    "request_separator": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "###"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[^\\r\\n]*"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "request": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "method_line"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "headers"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "method_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "method",
          "content": {
            "type": "SYMBOL",
            "name": "method"
          }
        },
        {
          "type": "PATTERN",
          "value": "[ \\t]+"
        },
        {
          "type": "FIELD",
          "name": "url",
          "content": {
            "type": "SYMBOL",
            "name": "target"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[ \\t]+"
                },
                {
                  "type": "FIELD",
                  "name": "version",
                  "content": {
                    "type": "SYMBOL",
                    "name": "http_version"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "method": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "GET"
        },
        {
          "type": "STRING",
          "value": "POST"
        },
        {
          "type": "STRING",
          "value": "PUT"
        },
        {
          "type": "STRING",
          "value": "DELETE"
        },
        {
          "type": "STRING",
          "value": "PATCH"
        },
        {
          "type": "STRING",
          "value": "OPTIONS"
        },
        {
          "type": "STRING",
          "value": "HEAD"
        },
        {
          "type": "STRING",
          "value": "TRACE"
        },
        {
          "type": "STRING",
          "value": "CONNECT"
        }
      ]
    },
    "target": {
      "type": "PATTERN",
      "value": "[^\\s\\r\\n]+"
    },
    "http_version": {
      "type": "PATTERN",
      "value": "HTTP\\/[0-9]+(\\.[0-9]+)?"
    },
    "headers": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "header"
      }
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "header_name"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "[ \\t]+"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "FIELD",
                  "name": "value",
                  "content": {
                    "type": "SYMBOL",
                    "name": "header_value"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    },
    "header_name": {
      "type": "PATTERN",
      "value": "[A-Za-z][\\w-]*"
    },
    "header_value": {
      "type": "PATTERN",
      "value": "[^\\r\\n]+"
    },
    "body": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        },
        {
          "type": "FIELD",
          "name": "content",
          "content": {
            "type": "SYMBOL",
            "name": "body_content"
          }
        }
      ]
    },
    "body_content": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n#]+"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "#"
                },
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^#\\r\\n]"
                  }
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]*"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "##"
                },
                {
                  "type": "IMMEDIATE_TOKEN",
                  "content": {
                    "type": "PATTERN",
                    "value": "[^#\\r\\n]"
                  }
                },
                {
                  "type": "PATTERN",
                  "value": "[^\\r\\n]*"
                },
                {
                  "type": "PATTERN",
                  "value": "\\r?\\n"
                }
              ]
            },
            {
              "type": "PATTERN",
              "value": "\\r?\\n"
            }
          ]
        }
      }
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{{"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "variable_name"
          }
        },
        {
          "type": "STRING",
          "value": "}}"
        }
      ]
    },
    "variable_name": {
      "type": "PATTERN",
      "value": "[^}]+"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "STRING",
              "value": "//"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]*"
        },
        {
          "type": "PATTERN",
          "value": "\\r?\\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    }
  ],
  "conflicts": [
    [
      "request"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}